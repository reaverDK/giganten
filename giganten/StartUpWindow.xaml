<Window x:Class="giganten.StartUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StartUpWindow" Height="300" Width="300" WindowStyle="None" AllowsTransparency="True" Visibility="Visible">
	<Window.Resources>
		<ResourceDictionary Source="SuperAwesomeStyle.xaml"></ResourceDictionary>
	</Window.Resources>

	<Grid>

		<Button Content="Button" HorizontalAlignment="Left" Margin="38,110,0,0" VerticalAlignment="Top" Width="75" FontWeight="Bold" Click="Button_Click"/>
		<Border BorderBrush="#BAB2C7D6" BorderThickness="20,2,20,4" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
		<Grid Background="#FF1E1A54" HorizontalAlignment="Stretch" Height="66" VerticalAlignment="Top" MouseDown="Grid_MouseDown">
			<Label Content="ELGIGANTEN" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="46" Width="280" Foreground="White" FontSize="36" FontFamily="Consolas" FontWeight="Bold"/>
		</Grid>
		<!-- Blue"#FF1E1A54" DarkGreen"#FF88F000" LightGreen"#FF8CE600" LightGray"#FFF0F0F0" BackgroundBlue"#FFA3B6C5"-->
	</Grid>
</Window>
