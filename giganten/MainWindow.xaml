<Window x:Class="giganten.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ElRank" Height="630" Width="930">
	<Window.Resources>
		<ResourceDictionary Source="SuperAwesomeStyle.xaml"></ResourceDictionary>
	</Window.Resources>
	<Grid Background="#FFECECEC">
		<DockPanel>
			<Menu DockPanel.Dock="Top" IsMainMenu="True">
				<MenuItem Header="_Filer">
					<MenuItem Header="_Åben..." Click="MenuItem_Click_Open"/>
					<MenuItem Header="_Reset" Click="MenuItem_Click_Reset"/>
					<MenuItem Header="_Eksportér som PDF" Click="MenuItem_Click_Export"/>
					<MenuItem Header="_Afslut" Click="MenuItem_Click_Exit"/>
				</MenuItem>
				<MenuItem Header="_Rediger"/>
				<MenuItem Header="_Hjælp">
					<MenuItem Header="_Kontakt information" Click="MenuItem_Click_Contact"/>
					<MenuItem Header="_Om ElRank" Click="MenuItem_Click_About"/>
				</MenuItem>
			</Menu>
			<ScrollViewer ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto">
				<TabControl Width="Auto" Background="#FFECECEC">
					<TabItem Header="Hovedside" >
						<Grid Background="#FFECECEC" Margin="0,0,0,0">
							<Label Content="Lønnummer:" HorizontalAlignment="Left" Margin="390,86,0,0" VerticalAlignment="Top" Height="26" Width="77"/>
							<TextBox x:Name="SANummerBox" HorizontalAlignment="Left" Height="22" Margin="481,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="79" ToolTip="F.eks. 123123"/>
							<Label Content="Lønnummer:" HorizontalAlignment="Left" Margin="590,86,0,0" VerticalAlignment="Top" Height="26" Width="77"/>
							<TextBox x:Name="SBNummerBox" HorizontalAlignment="Left" Height="22" Margin="681,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="79" ToolTip="F.eks. 123123"/>
							<Label x:Name="StatusBox" Content="Status: Ingen filer indlæst" HorizontalAlignment="Left" Margin="9,84,0,0" VerticalAlignment="Top" Width="170"/>
							<Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="270,117,9,9" Height="400" Width="600">
								<Border BorderBrush="Black" BorderThickness="1">
									<Canvas Height="200" Width="600" Name="graph_Person1">							
										<Canvas.RenderTransform>
											<TransformGroup>
												<ScaleTransform ScaleY="-1" />
												<TranslateTransform Y="200" />
											</TransformGroup>
										</Canvas.RenderTransform>
										<Rectangle Canvas.Left="40" Canvas.Top="31" Width="63" Height="41" Fill="Blue"  />
										<Ellipse Canvas.Left="130" Canvas.Top="79" Width="58" Height="58" Fill="Blue"  />
										<Line X1="0" Y1="0" X2="100" Y2="100" Stroke="Black" StrokeThickness="2" StrokeDashArray="5,3,1,1"/>
										<Line X1="100" Y1="100" X2="400" Y2="80" Stroke="Black" StrokeThickness="2" StrokeDashArray="5,3,1,1"/>
									</Canvas>
								</Border>
								<Border BorderBrush="Black" BorderThickness="1">
									<Canvas Height="200" Width="600" Margin="0,200,0,0" Name="graph_Person2">
										<Canvas.RenderTransform>
											<TransformGroup>
												<ScaleTransform ScaleY="-1" />
												<TranslateTransform Y="200" />
											</TransformGroup>
										</Canvas.RenderTransform>
										<Rectangle Canvas.Right="40" Canvas.Bottom="31" Width="63" Height="41" Fill="Green"  />
										<Ellipse Canvas.Right="130" Canvas.Bottom="79" Width="58" Height="58" Fill="Green"  />
										<Line X1="0" Y1="0" X2="100" Y2="100" Stroke="Black" StrokeThickness="2" StrokeDashArray="5,3"/>
										<Line X1="100" Y1="100" X2="400" Y2="150" Stroke="Black" StrokeThickness="2" StrokeDashArray="5,3"/>
										<Polyline Name="polyline1" Stroke="Red" StrokeThickness="2"></Polyline> 
									</Canvas>
								</Border>
							</Canvas>
							<ComboBox HorizontalAlignment="Left" Height="26" Margin="7,50,0,0" VerticalAlignment="Top" Width="170" SelectionChanged="ComboBox_SelectionChanged" SelectedIndex="0" Name="combobox_Afd">
								<ComboBoxItem Content="&lt;Ingen afdelinger valgt&gt;" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="Data" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="Hvidevare" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="Køkken" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="Små-EL" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="Tele" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="TV" HorizontalAlignment="Left" Width="168"/>
								<ComboBoxItem Content="ALLE AFDELINGER" HorizontalAlignment="Left" Width="168"/>
							</ComboBox>
							<ComboBox HorizontalAlignment="Left" Height="26" Margin="388,50,0,0" VerticalAlignment="Top" Width="170" SelectedIndex="0" Name="combobox_Person1">
								<ComboBoxItem Content="&lt;Ingen sælger valgt&gt;" HorizontalAlignment="Left" Width="168"/>
							</ComboBox>
							<ComboBox HorizontalAlignment="Left" Height="26" Margin="588,50,0,0" VerticalAlignment="Top" Width="170" SelectedIndex="0" Name="combobox_Person2">
								<ComboBoxItem Content="&lt;Ingen sælger valgt&gt;" HorizontalAlignment="Left" Width="168"/>
							</ComboBox>
							<Label Content="Vælg Afdeling :" HorizontalAlignment="Left" Height="35" Margin="5,10,0,0" VerticalAlignment="Top" Width="170"/>
							<Label Content="Vælg Sælger #1 :" HorizontalAlignment="Left" Height="35" Margin="388,10,0,0" VerticalAlignment="Top" Width="170"/>
							<Label Content="Vælg Sælger #2 :" HorizontalAlignment="Left" Height="35" Margin="593,10,0,0" VerticalAlignment="Top" Width="170"/>
							<CheckBox Content="Omsætning pr. Time" HorizontalAlignment="Left" Height="25" Margin="10,115,0,0" VerticalAlignment="Top" Width="136" Name="Omsætning"/>
							<CheckBox Content="Indtjening pr. Time" HorizontalAlignment="Left" Height="25" Margin="10,145,0,0" VerticalAlignment="Top" Width="136" Name="Indtjening"/>
							<CheckBox Content="Abonnement %" HorizontalAlignment="Left" Height="25" Margin="10,205,0,0" VerticalAlignment="Top" Width="136" Name="Abonnement"/>
							<CheckBox Content="Tilbehør %" HorizontalAlignment="Left" Height="25" Margin="10,175,0,0" VerticalAlignment="Top" Width="136" Name="Tilbehør"/>
							<CheckBox Content="Vægbeslag vs. TV" HorizontalAlignment="Left" Height="25" Margin="151,175,0,0" VerticalAlignment="Top" Width="114" Name="Vægbeslag_Vs_TV"/>
							<CheckBox Content="RTG Mobil Tele %" HorizontalAlignment="Left" Height="25" Margin="10,235,0,0" VerticalAlignment="Top" Width="136" Name="RTG_Mobil"/>
							<CheckBox Content="RTG IPad %" HorizontalAlignment="Left" Height="25" Margin="151,235,0,0" VerticalAlignment="Top" Width="114" Name="RTG_Ipad"/>
							<CheckBox Content="SA Aftaler %" HorizontalAlignment="Left" Height="25" Margin="151,205,0,0" VerticalAlignment="Top" Width="114" Name="SA_Aftaler"/>
							<CheckBox Content="Tryghedsaftale %" HorizontalAlignment="Left" Height="25" Margin="151,145,0,0" VerticalAlignment="Top" Width="114" Name="Tryghedsaftaler"/>
							<CheckBox Content="TDC TV Antal" HorizontalAlignment="Left" Height="25" Margin="151,115,0,0" VerticalAlignment="Top" Width="114" Name="TDC_TV"/>
							<CheckBox Content="VÆLG ALLE" HorizontalAlignment="Left" Height="25" Margin="84,266,0,0" VerticalAlignment="Top" Width="114" FontWeight="Bold" Name="Select_All" Checked="Select_All_Checked" Unchecked="Select_All_Unchecked"/>
						</Grid>
					</TabItem>
					<TabItem Header="Data filer">
						<Grid Background="#FFECECEC">
						</Grid>
					</TabItem>
				</TabControl>
			</ScrollViewer>
		</DockPanel>
	</Grid>

</Window>
