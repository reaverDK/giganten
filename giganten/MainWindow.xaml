<Window x:Class="giganten.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
	<Grid Background="#FFECECEC">
		<DockPanel>
			<Menu DockPanel.Dock="Top" IsMainMenu="True">
				<MenuItem Header="_File">
					<MenuItem Header="_Open"/>
					<MenuItem Header="_Close"/>
					<MenuItem Header="_Save"/>
					<MenuItem Header="_Print"/>
					<MenuItem x:Name="exitbutton" Header="_Exit" Click="MenuItem_Click"/>
				</MenuItem>
				<MenuItem Header="_Edit"/>
				<MenuItem Header="_Help">
					<MenuItem Header="_Contact Info"/>
					<MenuItem Header="_About Us"/>
				</MenuItem>
			</Menu>
			<TabControl Width="Auto" Background="#FFECECEC">
				<TabItem Header="Main" >
					<Grid Background="#FFECECEC">
						<Label Content="Sælgerkode:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,36,0,0"/>
						<Label Content="Lønnummer:" HorizontalAlignment="Left" Margin="0,62,0,0" VerticalAlignment="Top"/>
						<Label Content="Primær sælger:" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Width="201"/>
						<TextBox x:Name="SAKodeBox" HorizontalAlignment="Left" Height="23" Margin="91,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" ToolTip="F.eks. BOBBO"/>
						<TextBox x:Name="SANummerBox" HorizontalAlignment="Left" Height="23" Margin="91,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" ToolTip="F.eks. 123123"/>
						<Label Content="Sælgerkode:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="223,36,0,0"/>
						<Label Content="Lønnummer:" HorizontalAlignment="Left" Margin="223,62,0,0" VerticalAlignment="Top"/>
						<Label Content="Sælger til sammenligning:" HorizontalAlignment="Left" Margin="233,5,0,0" VerticalAlignment="Top" Width="201"/>
						<TextBox x:Name="SBKodeBox" HorizontalAlignment="Left" Height="23" Margin="314,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" ToolTip="F.eks. BOBBO"/>
						<TextBox x:Name="SBNummerBox" HorizontalAlignment="Left" Height="23" Margin="314,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" ToolTip="F.eks. 123123"/>
						<Label x:Name="SANavnBox" Content="Navn: " HorizontalAlignment="Left" Margin="0,93,0,0" VerticalAlignment="Top"/>
					</Grid>
				</TabItem>
				<TabItem Header="Other">
					<Grid Background="#FFECECEC">
					</Grid>
				</TabItem>
			</TabControl>
		</DockPanel>
	</Grid>
</Window>
